# Cloudflare Tunnel Configuration
# Get this token from Cloudflare Zero Trust dashboard after creating a tunnel
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

SUPABASE_PROJECT_ID=ID
SUPABASE_URL="https://${SUPABASE_PROJECT_ID}.supabase.co"

# BACKEND
DB_HOST=postgres
DB_PORT=5432
DB_USER=user
DB_PASSWORD=password
DB_NAME=weeate
SUPABASE_AUTH_URL="${SUPABASE_URL}/auth/v1/.well-known/jwks.json"
SUPABASE_API_KEY=sb_secret
SUPABASE_COOKIE_AUTH_NAME="sb-${SUPABASE_PROJECT_ID}-auth-token"

# Database Configuration (already set in docker-compose)
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=weeate

# FRONTEND - Supabase (Vite)
# Create a project at https://app.supabase.com and copy the values below
VITE_SUPABASE_URL="${SUPABASE_URL}"
VITE_SUPABASE_PUBLISHABLE_OR_ANON_KEY=sb_publishable
VITE_BACKEND_URL=http://localhost:8080/api
VITE_AUTH_COOKIE_NAME="${SUPABASE_COOKIE_AUTH_NAME}"